{
  "name": "slime",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf dist",
    "start": "npm run build && node dist/out.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "esbuild": "0.21.4",
    "eslint": "^8.57.0",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.8",
    "typescript": "5.4.5",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
